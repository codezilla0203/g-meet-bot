PORT=5000
AUTO_KILL_PORT_CONFLICT=1
BOT_HEADLESS=1
SERVE_STATIC_FRONTEND=false
ALLOWED_ORIGINS=
BASE_URL=http://localhost:5000
JWT_SECRET=Google-meet-bot
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o
OPENAI_TRANSCRIBE_MODEL=whisper-1
INACTIVITY_LIMIT=10               
ACTIVATE_INACTIVITY_AFTER=2      
AUDIO_SAMPLE_RATE=48000
AUDIO_FLUSH_INTERVAL_MS=5000
STREAM_WS_URL=ws://localhost:3000/ws/stream
TIMEZONE=America/Mexico_City
LOCALE=es-MX
FAST_VIDEO_MODE=true
ENABLE_VIDEO_COMPRESSION=true
VIDEO_BITRATE=90000  # 1.5 Mbps (smaller files, acceptable quality)
# Leader election / coordination
# Using local lock-files only (Redis support removed). Leave MEETING_LOCK_TTL_MS unset to use the built-in default.
# When true (default) bot will leave if it detects only other bot-like participants
# Set to "0" or "false" to disable
LEAVE_ON_ONLY_BOTS=1
# Heuristic detection tokens (comma-separated, case-insensitive)
# Add known platform/bot display-name tokens here, e.g. recall,fireflies,meetingbase,notetaker
BOT_IDENTIFIERS=recall,fireflies,otter,meetgeek,meetingbase,notetaker
# Tokens/domains to look for inside avatar image src/srcset (comma-separated)
BOT_IMAGE_DOMAINS=recall.ai,fireflies.ai,otter.ai,meetgeek.ai,tactiq.io
# Optional: restrict automatic leaving to only when lock acquisition fails
# (if you prefer to rely only on locks, set LEAVE_ON_ONLY_BOTS=0 above)
EMAIL_PROVIDER=
SMTP_HOST=
SMTP_PORT=
SMTP_SECURE=
EMAIL_USER=
EMAIL_PASSWORD=

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_S3_BUCKET=
AWS_REGION=
AWS_S3_DELETE_LOCAL_AFTER_UPLOAD=true
WEBHOOK_URL=